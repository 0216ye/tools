  "private": true,
  "scripts": {
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "dev": "vite --host",
    "devConfig:init": "node node/createDevHostConfig.js",
    "vite:dev": "vite ",
    "build:test": "cross-env BUILD_ENV=test NODE_OPTIONS=--max-old-space-size=7680 vite build",
    "build:report": "cross-env REPORT=true NODE_OPTIONS=--max-old-space-size=7680 vite build",
    "build": "npm run lint:all && rm -rf ./dist && cross-env NODE_OPTIONS=--max-old-space-size=7680 vite build",
    "vite:build": "npm run lint:all && rm -rf ./dist && cross-env NODE_OPTIONS=--max-old-space-size=7680 vite build",
    "test:build": "npm run lint:all && rm -rf ./dist && cross-env NODE_OPTIONS=--max-old-space-size=1536 vite build",
    "production:build": "npm run lint:all && rm -rf ./dist && cross-env NODE_OPTIONS=--max-old-space-size=7680 vite build",
    "typecheck": "vuedx-typecheck ./src",
    "lint": "npx eslint --cache --fix --ext .js,.ts,.vue,.jsx,.tsx src",
    "lint:style": "npx stylelint --cache \"src/**/*.{js,jsx,vue,less,scss,css}\" --fix",
    "lint:fn": "npx fs-lint --config fnlint.json",
    "lint:all": "npm run lint && npm run lint:style && npm run lint:fn",
    "postinstall": "husky install",
    "deploy": "bin/deploy.sh",
    "preview": "vite preview --port 3333"
  },
